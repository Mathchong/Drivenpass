generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int           @id @default(autoincrement())
  email       String        @unique
  password    String
  credentials credentials[]
}

model credentials {
  id       Int    @id @default(autoincrement())
  title    String
  url      String
  username String
  password String
  users    users  @relation(fields: [userId], references: [id])
  userId   Int

  @@unique([title, userId])
}
